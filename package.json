{
  "name": "cid-lang",
  "version": "1.0.0",
  "description": "A varient of the scheme language",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "cidlang": "dist/cli.js"
  },
  "scripts": {
    "tsup": "tsup",
    "dev": "npm run build -- --watch --onSuccess \"npm run start\"",
    "build": "tsup src/index.ts src/repl.ts src/cli.ts --shims --dts --sourcemap --format esm,cjs --keep-names --treeshake recommended",
    "test": "jest",
    "coverage": "npm test -- --coverage --collectCoverageFrom=\"./src/**\"",
    "start": "node dist/repl.js",
    "cli": "node dist/cli.js",
    "format": "npx prettier --write .",
    "lint": "npx eslint .",
    "verify": "npm run test && npm run lint && npm run build"
  },
  "author": "Kylee Tilley",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "expect": "^27.5.1",
    "jest": "^27.5.1",
    "prettier": "^2.8.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsup": "^6.0.1",
    "typescript": "^4.9.5"
  }
}
